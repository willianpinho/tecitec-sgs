#extend("embeds/base")

#export("title") { Tecitec - Clientes }

#export("content") {
<div class="page-content">
    <div class="container-fluid">
        
        
        <header class="section-header">
            <div class="tbl">
                <div class="tbl-row">
                    <div class="tbl-cell">
                        <h3>Clientes</h3>
                        <ol class="breadcrumb breadcrumb-simple">
                            <li><a href="">Tecitec</a></li>
                            <li class="active">Clientes</li>
                        </ol>
                    </div>
                </div>
            </div>
        </header>
        <div class="box-typical box-typical-padding">
            <ul class=" ">
                <form id="form-group" role="form" autocomplete="on" action="/clientes/#(cliente.id)/editar" method="get">
                    <li><button type="submit" class="btn btn-inline pull-right btn-warning-outline">Editar Cliente</button></li>
                </form>
                
            </ul>
            <h5 class="m-t-lg with-border">Dados de Cliente</h5>
            
                <div class="form-group row">
                    <label class="col-sm-2 form-control-label">Nome</label>
                    <div class="col-sm-10">
                        <p class="form-control-static">#(cliente.nome)</p>
                    </div>
                </div>
                <div class="form-group row">
                    <label class="col-sm-2 form-control-label">Email</label>
                    <div class="col-sm-10">
                        <p class="form-control-static">#(cliente.email)</p>
                    </div>
                </div>
                <div class="form-group row">
                    <label class="col-sm-2 form-control-label">Tipo Cliente</label>
                    <div class="col-sm-10">
                        <p class="form-control-static">#(cliente.tipo.nome)</p>
                    </div>
                </div>
                <div class="form-group row">
                    <label class="col-sm-2 form-control-label">Documento</label>
                    <div class="col-sm-10">
                        <p class="form-control-static">#(cliente.documento)</p>
                    </div>
                </div>
                <div class="form-group row">
                    <label class="col-sm-2 form-control-label">Endereço</label>
                    <div class="col-sm-10">
                        <p class="form-control-static">#(cliente.endereco)</p>
                    </div>
                </div>
                <div class="form-group row">
                    <label class="col-sm-2 form-control-label">Complemento</label>
                    <div class="col-sm-10">
                        <p class="form-control-static">#(cliente.complemento)</p>
                    </div>
                </div>
                <div class="form-group row">
                    <label class="col-sm-2 form-control-label">CEP</label>
                    <div class="col-sm-10">
                        <p class="form-control-static">#(cliente.cep)</p>
                    </div>
                </div>
                <div class="form-group row">
                    <label class="col-sm-2 form-control-label">Cidade</label>
                    <div class="col-sm-10">
                        <p class="form-control-static">#(cliente.cidade.nome)</p>
                    </div>
                </div>
                <div class="form-group row">
                    <label class="col-sm-2 form-control-label">Celular</label>
                    <div class="col-sm-10">
                        <p class="form-control-static">#(cliente.celular)</p>
                    </div>
                </div>
                <div class="form-group row">
                    <label class="col-sm-2 form-control-label">Telefone</label>
                    <div class="col-sm-10">
                        <p class="form-control-static">#(cliente.telefone)</p>
                    </div>
                </div>
                <div class="form-group row">
                    <label class="col-sm-2 form-control-label" for="date-mask-input">Data de Nascimento</label>
                    <div class="col-sm-10">
                        <p class="form-control-static">#(cliente.data_nascimento)</p>
                            </div>
                </div>
                <div class="form-group row">
                    <label class="col-sm-2 form-control-label">Observações</label>
                    <div class="col-sm-10">
                        <p class="form-control-static">#(cliente.observacoes)</p>
                    </div>
                </div>
                

                
            
            
        </div><!--.box-typical-->
        
    </div><!--.container-fluid-->
</div><!--.page-content-->
}

